{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "homepage": "/modia",
  "proxy": "http://localhost:8080",
  "dependencies": {
    "classnames": "2.2.5",
    "nav-frontend-core": "4.0.5",
    "nav-frontend-ikoner-assets": "0.2.3",
    "nav-frontend-lenker-style": "0.2.1",
    "nav-frontend-spinner": "0.1.3",
    "nav-frontend-spinner-style": "0.2.1",
    "nav-frontend-typografi": "1.0.2",
    "nav-frontend-typografi-style": "1.0.1",
    "query-string": "5.0.0",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-intl": "2.3.0",
    "react-scripts": "1.0.11",
    "prop-types": "15.5.10"
  },
  "scripts": {
    "copyfiles": "copyfiles -u 1 build/**/*.* ../main/webapp",
    "delete-static": "node scripts/deletestatic.js",
    "build-css": "lessc src/index.less -x src/index.css",
    "watch-css": "autoless --interval 100 src src",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run delete-static && npm run build-css && react-scripts build && npm run copyfiles",
    "test-react-scripts": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "eslint": "eslint --fix ./src/",
    "test": "mocha"
  },
  "devDependencies": {
    "autoless": "0.1.7",
    "copyfiles": "1.2.0",
    "eslint": "4.4.1",
    "eslint-config-airbnb": "15.1.0",
    "eslint-import-resolver-node": "0.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "fetch-mock": "5.12.1",
    "less": "2.7.2",
    "mocha": "3.5.0",
    "npm-run-all": "4.0.2",
    "path-to-regexp": "1.7.0"
  }
}
